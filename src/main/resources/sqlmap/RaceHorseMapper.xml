<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.majin.bit.dao.RaceHorseDao">

	<insert id="raceHorseInsert" parameterType="java.util.Map" keyProperty="raceHrno">
		INSERT INTO RACEHORSE(HRNO, JKNO, TRNO, RCDATE, MEET)
		VALUES
		<foreach collection="list" item="item" index="index" separator=",">
			(#{item.hrNo}, #{item.jkNo}, #{item.trNo}, #{item.rcDate}, #{item.meet})
		</foreach>

	</insert>
</mapper>